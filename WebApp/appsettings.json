{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ConnectionStrings": {
    "Garda": "Server=127.0.0.1; Port=5432; Database=garda; User Id=postgres; Password=; MaxPoolSize=100; CommandTimeout=300;",
    "Rabbit": "amqp://garda:123asdZXC$@192.168.38.109:34615"
  },
  "AllowedHosts": "*",
  "NLog": {
    "autoReload": true,
    "throwExceptions": true,
    "targets": {
      "MyFile": {
        "type": "File",
        "fileName": "logs/somefolder/logs.log",
        "layout": "${longdate:universalTime=true} ${callsite} ${uppercase:${level}} ${message}"
      }
    },
    "rules": [
      {
        "logger": "*",
        "writeTo": "MyFile",
        "minLevel":  "Debug"
      }
    ]
  }
}
